<h2>Shared Repositories / <%= description %></h2>

<div class="repos">
<% sharedRepos.forEach(function(repo) { %>
  <div class="repo">
    <% var stars = ''; for (var i = 0; i < repo.watchers.length; i++) { stars += '*'; } %>
    <span class="details"><%= repo.language %>&nbsp;&nbsp;&nbsp;&nbsp; â˜… <%= repo.watchers.length %></span>
    <h3>http://<%= host %>:<%= port %>/<%= repo.name %> <%= stars %></h3>
    <h4><%= repo.description %></h4>
  </div>
<% }); %>
</div>
